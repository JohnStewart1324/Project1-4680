{
  "name": "stockie-web",
  "version": "1.0.0",
  "description": "Stockie - AI-Powered Practice Trading Simulator",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:vite\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:vite": "vite",
    "dev:web": "npm run dev",
    "dev:electron": "wait-on http://localhost:5173 && electron .",
    "dev:desktop": "concurrently \"npm run dev:server\" \"npm run dev:vite\" \"npm run dev:electron\"",
    "build": "tsc -p tsconfig.electron.json && vite build && tsc -p tsconfig.electron.json",
    "build:web": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "start": "npm run build && electron .",
    "start:web": "npm run build:web && npm run preview",
    "start:server": "npm run build:server && node dist/server/index.js",
    "electron": "electron .",
    "server": "tsx server/index.ts",
    "deploy": "npm run build:web && vercel --prod"
  },
  "keywords": [
    "web",
    "react",
    "typescript",
    "stock",
    "trading",
    "simulator",
    "ai",
    "finance"
  ],
  "author": "Stock Viewer App",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vercel": "^33.0.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0"
  }
}
